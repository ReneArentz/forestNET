    <xs:element name="result" type="xs:double" fAny:mapping="result"/>
    
    <xs:complexType name="Add" fAny:mapping="Sandbox.Tests.Net.Https.Calculator+Add, Sandbox">
        <xs:sequence>
            <xs:element ref="param_1"/>
