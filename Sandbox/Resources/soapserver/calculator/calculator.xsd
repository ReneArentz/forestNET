<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:fAny="http://www.forestany.net" xsi:schemaLocation="http://www.forestany.net ./forestAnyXML.xsd" elementFormDefault="qualified" attributeFormDefault="unqualified" targetNamespace="http://www.forestany.net">
