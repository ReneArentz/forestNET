    <xs:complexType name="MultiplyResult" fAny:mapping="Sandbox.Tests.Net.Https.Calculator+MultiplyResult, Sandbox">
        <xs:sequence>
            <xs:element ref="result"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="Divide" fAny:mapping="Sandbox.Tests.Net.Https.Calculator+Divide, Sandbox">
        <xs:sequence>
            <xs:element ref="param_1"/>
            <xs:element ref="param_2"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="DivideResult" fAny:mapping="Sandbox.Tests.Net.Https.Calculator+DivideResult, Sandbox">
